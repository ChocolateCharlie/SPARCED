process {
  container = 'ebensma/sparced:latest'
	shell = ['/bin/bash','-e']
}

docker.enabled = true

k8s {
	storageClaimName = "deepgtex-prp"
	storeageMountPath = "/workspace/ethan"
	launchDir = "/workspace/ethan"
  workDir = "/workspace/ethan/work"
}

params {
  input_dir = '/workspace/ethan/input_data/'
  sweep = ''
  deterministic = 1
  time = 2
  feedTime = 30
  numCells = 1
  Vn = '1.75E-12'
  Vc = '5.25E-12'
  speciesVals = 'EGF:100,Heregulin:100.0,HGF:100.0,PDGF:100.0,FGF:100.0,IGF:100.0,INS:1721.0'

}
