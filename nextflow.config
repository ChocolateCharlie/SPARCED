process {
  container = 'ebensma/sparced:latest'
	shell = ['/bin/bash','-e']
}

docker.enabled = true

k8s {
	storageClaimName = "deepgtex-prp"
	storeageMountPath = "/workspace/ebensman"
	launchDir = "/workspace/ebensman"
  workDir = "/workspace/ebensman/work"
}

params {
  input_dir = '/workspace/ebensman/input_data/'
  sweep = 'species:m_PDGFB:IC_Xinitialized:0,0.395'
}
